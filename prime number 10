#include <stdio.h>
int main()
{
    int a,i;
    printf("/n Enter the  number ; ");
    scanf("%d",&a);
    for(i =2;i <=a/2;i++)
    {
        if (a%i==0)
        {
            break;
        }
    }
    if (i>a/2)
    printf("%d is a prime number/n",a);
    else
    printf("/n%d is not aprimr number/n",a);
    return 0;
}
